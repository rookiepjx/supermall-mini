/* components/w-tabcontrol/w-tabcontrol.wxss */
.tabcontrol{
  padding: 10rpx;
  display: flex;
  background-color: #fff;

}
.tabcontrol-item{
  flex:1;
  text-align: center;
  font-size: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.tabcontrol-item.active .text{
  padding: 16rpx 10rpx;
  color:#ff5777;
  border-bottom: 6rpx solid #ff5777;
}